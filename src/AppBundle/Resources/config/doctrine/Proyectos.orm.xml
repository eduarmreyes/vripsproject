<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Proyectos" table="proyectos">
    <id name="idProyecto" type="integer" column="id_proyecto">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idMatriz" type="integer" column="id_matriz" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idPerfil" type="integer" column="id_perfil" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idCronograma" type="integer" column="id_cronograma" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
